<!--抢占成功-->
<view class="container">
  <view class="content-wrap">
    <icon class="icon-box-img" type="success" size="22"></icon>
    <view class="success-title">预定成功</view>
  </view>
  <view class="success-content">
    <view class="title">{{successInfo.subject}}</view>
    <view class="info-wrap">
      <view class="address">
        <view class="label">会议地点</view>
        <view class="info">{{successInfo.name}}（{{successInfo.uses_name}} 预计{{successInfo.capacity}}人）</view>
      </view>
      <view class="time">
        <view class="label">会议时间</view>
        <view class="info">{{successInfo.date}} {{successInfo.metting_start_time}}-{{successInfo.metting_end_time}}</view>
      </view>
    </view>
  </view>

  <view class="btn-group">
    <view class="btn-wrap">
      <button class="btn-sure" type="primary" size="default" style="width: 100%;margin-bottom: 30rpx;">编辑会议信息</button>
    </view>
  </view>

</view>
