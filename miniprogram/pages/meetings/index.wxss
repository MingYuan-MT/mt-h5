/**index.wxss**/
.container {
  width: 100%;
}
.w-form-item {
  width: 100%;
  height: 50px;
  line-height: 50px;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: right;
  border-bottom: 1px solid #d9d9d9;
}
.w-form-label {
  width: 110px;
}
.w-form-content {
  flex: 1;
  padding: 0;
  text-align: left;
  padding: 0 10px 0 30px;
}

/* 按钮 */
.btn-group {
  width: 100%;
  padding: 80rpx 0;
}
.btn-wrap {
  padding: 0 30rpx;
}
